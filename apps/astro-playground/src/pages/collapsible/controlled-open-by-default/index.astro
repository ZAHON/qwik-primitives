---
import { BaseLayout } from '@/layouts';
import { ControlledOpenByDefault } from '@/demos/collapsible/controlled-open-by-default';

const title = 'Collapsible controlled open by default | Qwik Primitives Astro Playground';
const description = 'An interactive component which expands/collapses a panel.';
---

<BaseLayout title={title} description={description}>
  <a href="/collapsible">
    <h1>Collapsible</h1>
  </a>
  <p>An interactive component which expands/collapses a panel.</p>
  <ControlledOpenByDefault />
</BaseLayout>
