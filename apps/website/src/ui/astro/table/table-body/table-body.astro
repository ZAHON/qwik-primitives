---
import type { HTMLAttributes } from 'astro/types';
import { twm } from '@/utilities/twm';
import { tableBodyStyles } from './table-body.styles';

type Props = HTMLAttributes<'tbody'>;

const { class: className, ...others } = Astro.props;
---

<tbody class={twm(tableBodyStyles(), className)} {...others}>
  <slot />
</tbody>
