---
import { docsNavListStyles } from './docs-nav-list.styles';

interface Props {
  /**
   * The unique id of the heading that labels the element.
   */
  'aria-labelledby': string;
}

const { 'aria-labelledby': ariaLabelledby } = Astro.props;
---

<ul aria-labelledby={`docs-nav-${ariaLabelledby}`} class={docsNavListStyles()}>
  <slot />
</ul>
