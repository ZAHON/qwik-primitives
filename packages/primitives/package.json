    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "qwik": "qwik",
    "start": "vite --open --mode ssr",
    "test": "vitest run",
    "test:watch": "vitest --reporter verbose",
    "clean": "rm -rf .turbo node_modules lib lib-types"
  },
