  "scripts": {
    "build": "pnpm run lint && pnpm run build:clean && pnpm run build:types && pnpm run build:lib",
    "build:lib": "vite build --mode lib",
    "build:types": "tsc --emitDeclarationOnly --project tsconfig.build.json",
